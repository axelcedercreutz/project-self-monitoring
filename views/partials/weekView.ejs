<form method="POST" action="/behavior/summary/week">
    <div style="width:100%">
        <p>Date</p>
        <input type="week" name="week" value="2020-W<%= week %>"/>
    </div>
    <input type="submit" value="Change week!" />
</form>

<% if(weekSummary.length > 0) { %>
<h2>Week:<%= weekSummary[0].week %> </h2>
<table>
    <thead>
        <tr>
            <th>Average sleep quality</th>
            <th>Average sleep dration</th>
            <th>Average study dration</th>
            <th>Average exercise dration</th>
            <th>Average mood</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td><%= weekSummary[0].avg_sleepquality %></td>
            <td><%= weekSummary[0].avg_sleepduration %></td>
            <td><%= weekSummary[0].avg_study %></td>
            <td><%= weekSummary[0].avg_exercise %></td>
            <td><%= weekSummary[0].avg_mood %></td>
        </tr>
    </tbody>
</table>
<% } else { %>
    <h2>No data for selected week </h2>
<% } %>